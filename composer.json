        {
            "name": "Christian Dilger",
            "email": "c.dilger@addorange.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "typo3/cms-core": "~7.6.0 || ~8.7.0",
        "rkw/rkw-basics": "~8.7.80",
        "rkw/rkw-mailer": "~9.5.9",
        "rkw/rkw-registration": "~8.7.0"
