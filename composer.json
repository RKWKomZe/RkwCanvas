{
    "name": "rkw/rkw-canvas",
    "type": "typo3-cms-extension",
    "description": "Extension for using a digital canvas to work on ideas",
    "homepage": "https://www.rkw-kompetenzzentrum.de",
    "license": ["GPL-2.0+"],
    "keywords": ["TYPO3 CMS", "RKW", "Canvas"],
    "support": {
        "email": "c.dilger@addorange.de"
    },
    "authors": [
        {
            "name": "Christian Dilger",
            "email": "c.dilger@addorange.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
		"ext-json": "*",
        "typo3/cms-core": "~10.4.0",
        "madj2k/t3-core-extended": "~10.4.0 || ~11.5.0 || ~12.4.0",
		"madj2k/t3-fe-register": "~10.4.0 || ~11.5.0 || ~12.4.0",
        "rkw/rkw-basics": "~10.4.0 || ~11.5.0 || ~12.4.0"
    },
    "autoload": {
        "psr-4": {
            "RKW\\RkwCanvas\\": "Classes/"
        }
